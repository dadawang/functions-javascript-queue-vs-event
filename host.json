{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "sampling": {
        "isEnabled": false
      }
    }
  },
  "extensions": {
    "serviceBus": {
      "messageHandlerOptions": {
          "maxConcurrentCalls": 16
      }
    },
    "queues": {
      "batchSize": 16,
      "newBatchThreshold": 8
    }
  }
}
